<!DOCTYPE html>
<html>
  <head>
    <title>Search Results</title>
  </head>
  <body>
    <h1>Search Results</h1>

    <% if (results.length > 0) { %>
      <% results.forEach(result => { %>
        <div>
          <h2><%= result.title %></h2>
          <p><%= result.content %></p>
        </div>
      <% }) %>
    <% } else { %>
      <p>No results found</p>
    <% } %>
  </body>
</html>
